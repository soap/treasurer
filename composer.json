{
    "name": "soap/treasurer",
    "description": "Laravel integration for OMISE payment service",
    "keywords": [
        "soap",
        "laravel",
        "treasurer"
    ],
    "homepage": "https://github.com/soap/treasurer",
    "license": "MIT",
    "authors": [
        {
            "name": "Prasit Gebsaap",
            "email": "prasit.gebsaap@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0",
        "omise/omise-php": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.18",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Soap\\Treasurer\\": "src",
            "Soap\\Treasurer\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\Treasurer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soap\\Treasurer\\TreasurerServiceProvider"
            ],
            "aliases": {
                "Treasurer": "Soap\\Treasurer\\Facades\\Treasurer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
